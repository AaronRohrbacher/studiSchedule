<%= form_for [@school, @event, @schedule] do |f| %>
<%= f.label :day %>
<%= f.text_field :day %>

<%= f.label :start_time %>
<%= f.time_select :start_time %>

<%= f.label :end_time %>
<%= f.time_select :end_time %>

<% options = options_from_collection_for_select(@rooms, 'id', 'name') %>
<%= f.select :room_id, options %>

<%= f.submit "submit" %>
<% end %>
