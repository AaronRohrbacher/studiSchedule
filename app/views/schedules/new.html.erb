<%= form_for [@school, @event, @schedule] do |f| %>

<%= f.label :start_time %>
<%= f.time_select :start_time, {minute_step: 15} %>

<%= f.label :end_time %>
<%= f.time_select :end_time, {minute_step: 15}%>

<%= f.label :date %>
<%= f.date_field :date %>

<%= f.label :recurring %>
<%= f.text_field :recurring %>

<% options = options_from_collection_for_select(@rooms, 'id', 'name') %>
<%= f.select :room_id, options %>

<%= f.submit "submit" %>
<% end %>
